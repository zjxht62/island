<definitions id="definitions"
             targetNamespace="http://activiti.org/bpmn20"
             xmlns:activiti="http://activiti.org/bpmn"
             xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL">

  <process id="trustOperation" name="Try to call an interface process">

    <startEvent id="theStart" />

    <sequenceFlow id='flow1' sourceRef='theStart' targetRef='applyTask' />

    <userTask id="applyTask" name="Apply for call an interface" >
      <documentation>
        Apply for call an interface.
      </documentation>
      <potentialOwner>
        <resourceAssignmentExpression>
          <formalExpression>accountancy</formalExpression>
        </resourceAssignmentExpression>
      </potentialOwner>
    </userTask>

    <sequenceFlow id='flow2' sourceRef='applyTask' targetRef='verifyTask' />

    <userTask id="verifyTask" name="Verify the parameters" >
      <documentation>
        Verify the parameters
      </documentation>
      <potentialOwner>
        <resourceAssignmentExpression>
          <formalExpression>management</formalExpression>
        </resourceAssignmentExpression>
      </potentialOwner>
    </userTask>

    <sequenceFlow id='flow3' sourceRef='verifyTask' targetRef='theEnd' />

    <endEvent id="theEnd" />

  </process>

</definitions>